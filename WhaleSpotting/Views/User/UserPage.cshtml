@model WsUser

@{
    ViewData["Title"] = "My User Page";
}
<h1>@ViewData["Title"]</h1>

<p>My Sightings.</p>

<div>
    
        <ul class="card">
            <div class="card-body"></div>
                @foreach (var sighting in @Model.Sightings)
                {
                    <li>
                        <h4 class="card-title">@sighting.Date</h4>
                        <p class="card-text"> Latitute: @sighting.Latitude Longitude: @sighting.Longitude</p>
                        <p class="card-text"> Description of Sighting:</p>
                        <p class="card-text"> @sighting.Description</p>
                    </li>
                }
            </div>
        </ul>
    
</div>
